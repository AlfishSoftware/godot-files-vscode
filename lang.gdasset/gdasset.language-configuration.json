{
  "comments": {
    "lineComment": ";",
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    { "open": "\"", "close": "\"", "notIn": ["string"] },
  ],
  "autoCloseBefore": ";,}]) \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
  ],
  "wordPattern": "\\b(?:Ext|Sub)Resource\\s*\\(\\s*\\d+\\s*\\)|(?<![.])-?\\b\\d+(?:\\.\\d+)?(?:e[-+]?\\d+)?\\b(?=$|[\\s#;,}\\]):=])|\\b[\\w-]+\\b",
}
