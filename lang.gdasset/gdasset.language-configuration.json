{
  "comments": {
    "lineComment": ";",
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    { "open": "\"", "close": "\"", "notIn": ["string"] },
  ],
  "autoCloseBefore": ";,}]) \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
  ],
  "folding": {
    "markers": {
      "start": "^(?:code|script/source)[ \\t]*=[ \\t]*\"",
      "end": "^[^\\\\]*\"",
    },
  },
  "wordPattern": "(?<=\")res://[^\"\\\\]*(?=\")|(?<=^\\[\\s*ext_resource\\s+path\\s*=\\s*\")[^\"\\\\]*(?=\")|\\b(?:Ext|Sub)Resource\\s*\\(\\s*\\d+\\s*\\)|(?<![.])-?\\b\\d+(?:\\.\\d+)?(?:e[-+]?\\d+)?\\b(?=$|[\\s#;,}\\]):=])|\\b[\\w-]+\\b",
}
