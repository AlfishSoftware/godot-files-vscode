{
  "name": "godot-files",
  "version": "0.0.1",
  "displayName": "Godot Files",
  "description": "Provides syntax-highlighting for some files supported by Godot Editor.",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlfishSoftware/godot-files-vscode"
  },
  "bugs": {
    "url": "https://github.com/AlfishSoftware/godot-files-vscode/issues"
  },
  "icon": "godot.png",
  "keywords": [
    "godot",
    "gdshader",
    "shader",
    "shading",
    "gd",
    "asset",
    "config",
    "properties",
    "ini",
    "freedesktop",
    "xdg"
  ],
  "publisher": "alfish",
  "license": "0BSD",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "type": "commonjs",
  "main": "./src/extension.js",
  "activationEvents": [
    "onLanguage:gdasset",
    "onLanguage:config-definition"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gdshader",
        "aliases": [
          "Godot Shader",
          "Godot Shading Language",
          "GDShader"
        ],
        "extensions": [
          ".gdshader"
        ],
        "icon": {
          "dark": "./lang.gdshader/gdshader.svg",
          "light": "./lang.gdshader/gdshader.svg"
        },
        "mimetypes": [
          "application/x-godot-shader"
        ],
        "firstLine": "^\\s*shader_type\\s+(spatial|canvas_item|particles)\\s*;\\s*(//.*)?$",
        "configuration": "./lang.gdshader/gdshader.language-configuration.json"
      },
      {
        "id": "gdasset",
        "aliases": [
          "Asset Properties Definition",
          "Godot Asset",
          "GDAsset"
        ],
        "filenames": [
          "project.godot"
        ],
        "extensions": [
          ".cfg",
          ".tres",
          ".tscn",
          ".escn",
          ".gdns",
          ".gdnlib",
          ".import"
        ],
        "icon": {
          "dark": "./lang.gdasset/godot.dark.svg",
          "light": "./lang.gdasset/godot.light.svg"
        },
        "mimetypes": [
          "application/x-godot-project",
          "application/x-godot-resource",
          "application/x-godot-scene"
        ],
        "firstLine": "^\\[(remap|preset\\.\\d+|gd_(scene|resource type=\".*?\") load_steps=\\d+ format=\\d+)\\]$",
        "configuration": "./lang.gdasset/gdasset.language-configuration.json"
      },
      {
        "id": "config-definition",
        "aliases": [
          "Configuration Properties",
          "Desktop Entry"
        ],
        "extensions": [
          ".desktop",
          ".directory"
        ],
        "firstLine": "^\\[Desktop Entry\\]$",
        "configuration": "./lang.config-definition/config-definition.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gdshader",
        "scopeName": "source.gdshader",
        "path": "./lang.gdshader/gdshader.tmLanguage.json"
      },
      {
        "language": "gdasset",
        "scopeName": "source.gdasset",
        "path": "./lang.gdasset/gdasset.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.gdshader": "gdshader",
          "meta.embedded.block.gdscript": "gdscript"
        }
      },
      {
        "language": "config-definition",
        "scopeName": "source.gdasset",
        "path": "./lang.gdasset/gdasset.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "gdshader",
        "path": "./lang.gdshader/gdshader.code-snippets"
      },
      {
        "language": "gdasset",
        "path": "./lang.gdasset/gdasset.code-snippets"
      },
      {
        "language": "config-definition",
        "path": "./lang.config-definition/config-definition.code-snippets"
      }
    ],
    "problemMatchers": [
    ],
    "commands": [
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/vscode": "^1.66.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm prune --production && npm run compile"
  }
}
