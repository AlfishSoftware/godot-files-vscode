{
  "comments": {
    "lineComment": "//",
    "blockComment": [ "/*", "*/" ],
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    { "open": "/*", "close": " */", "notIn": ["string"] },
  ],
  "autoCloseBefore": ";,}]) \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
  ],
  "folding": {
    "markers": {
      "start": "^[ \\t]*//#region\\b",
      "end": "^[ \\t]*//#endregion\\b",
    },
  },
}
